<!-- header top area -->
<div class="cleanmaster-header-top">
    <div class="container">
        <!-- STYLE 1 RIGHT ICON  -->
        <div class="row">
            <!-- TOP LEFT -->
            <div class="col-lg-5 col-md-4 col-sm-7">
                <div class="top-address">
                    <p>
                        <a href="tel:@company.PhoneNo"><i class="icofont-phone"></i>@company.PhoneNo</a>
                        <a href="mailto:@company.Email"><i class="icofont-envelope"></i>@company.Email</a>
                    </p>
                </div>
            </div>
            <div class="col-lg-3 col-md-4 col-sm-5">
                <div class="top-welcome">
                    <p class="text-center">

                        <span>
                            <i class="icofont-heart-alt"></i>
                            @company.WelcomeText
                        </span>
                    </p>
                </div>
            </div>
            <!-- TOP RIGHT -->
            <div class="col-lg-4 col-md-4 col-sm-12">
                <div class="top-right-menu">
                    <ul class="social-icons text-right text_m_center">

                        @if (companyProfileList.Count > 0)
                        {
                            foreach (var profile in companyProfileList)
                            {
                                <li><a href="@profile.ProfileLink"><i class="icofont-@profile.ProfileIcon"></i></a></li>
                            }

                        }
                        else
                        {
                            <li><a href="#"><i class="icofont-facebook"></i></a></li>
                            <li><a href="#"><i class="icofont-twitter"></i></a></li>
                            <li><a href="#"><i class="icofont-instagram"></i></a></li>
